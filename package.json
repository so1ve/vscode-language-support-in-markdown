{
  "private": true,
  "scripts": {
    "build": "nr --filter \"./packages/*\" build",
    "lint": "eslint . && prettier . --check",
    "lint:fix": "eslint . --fix && prettier . --write",
    "pack": "nr --filter=\"./packages/vscode\" pack",
    "prerelease": "nr build",
    "release": "bumpp && nr --filter=\"./packages/vscode\" publish",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.4",
    "@so1ve/eslint-config": "^0.113.3",
    "@so1ve/prettier-config": "^0.115.0",
    "@types/node": "^18.16.6",
    "bumpp": "^9.1.1",
    "eslint": "^8.45.0",
    "pkgroll": "^1.10.0",
    "prettier": "^3.0.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  }
}
