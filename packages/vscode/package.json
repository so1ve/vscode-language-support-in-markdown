    "publish": "vsce publish --no-dependencies",
    "vscode:prepublish": "nr build"
  },
  "devDependencies": {
    "@html1/language-server": "workspace:*",
    "@types/vscode": "1.78.1",
    "@volar/language-server": "~1.9.0",
    "@volar/vscode": "~1.9.0",
    "@vscode/vsce": "latest",
    "vscode-languageclient": "^8.1.0"
